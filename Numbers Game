import random
def game():
    print("You are playing the game:")
    score = random.randint(1, 100)
    print(f"Your score is {score}")
    return score

score = game()
with open("hiscore.txt", "r+") as f:
    x= f.read()
    if x!="":
        x=int(x)
    else:
        x=0
if x < score:
    with open("hiscore.txt", "w") as f:
        f.write(str(score))
        print("That's a high score ")
